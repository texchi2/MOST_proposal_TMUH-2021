@article{Tsai2012,
abstract = {Colorectal cancer (CRC) is one of the most common cancers worldwide. More than half of all CRC patients will develop metastases, which represents the major cause of death for CRC patients. CRC metastases confined in other organs are potentially resectable, and patients who receive curative resections appear to have better outcomes. Thus, the early detection of metastasis in CRC patients could improve their survival rate after curative surgery. Here, we report the use of Cy-dye labeling combined with multi-dimensional fractionation and mass spectrometry as a proteomics-based approach for identifying CRC metastasis-associated biomarker(s) in plasma samples collected from three CRC patients upon diagnosis of their primary and metastatic tumors. Among the eight identified proteins, we used Western blot analysis and an in-house-developed ELISA to validate the increased plasma levels of one, secretory (plasma) gelsolin, in {\textgreater}80{\%} of CRC patients with distal metastases in a larger sample cohort (32 patients). We also found a significant increase of secretory gelsolin in plasma samples of stage IV versus stages I-III CRC patients before treatment. Furthermore, immunohistochemistry showed that secretory gelsolin was highly overexpressed in CRC tissue specimens compared to adjacent normal tissues, and a cell model study showed that secretory gelsolin may help regulate CRC cell migration.},
author = {Tsai, Ming Hung and Wu, Chih Ching and Peng, Pei Hua and Liang, Ying and Hsiao, Yung Chin and Chien, Kun Yi and Chen, Jeng Ting and Lin, Shin Jie and Tang, Rei Ping and Hsieh, Ling Ling and Yu, Jau Song},
doi = {10.1007/s00109-011-0817-4},
file = {:Users/texchi/Library/Application Support/Mendeley Desktop/Downloaded/Tsai et al. - 2012 - Identification of secretory gelsolin as a plasma biomarker associated with distant organ metastasis of colorectal c.pdf:pdf},
isbn = {0010901108},
issn = {09462716},
journal = {Journal of Molecular Medicine},
keywords = {Colorectal cancer,Metastasis,Migration,Plasma biomarker,Secretory gelsolin},
number = {2},
pages = {187--200},
pmid = {21997591},
title = {{Identification of secretory gelsolin as a plasma biomarker associated with distant organ metastasis of colorectal cancer}},
volume = {90},
year = {2012}
}
