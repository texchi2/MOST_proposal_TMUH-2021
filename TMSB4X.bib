issn = {0021-9533},
journal = {Journal of cell science},
keywords = {Actins,Actins: chemistry,Actins: metabolism,Active Transport,Animals,Binding Sites,Cadaverine,Cadaverine: pharmacology,Carboxylic Acids,Carboxylic Acids: pharmacology,Cell Line,Cell Nucleus,Cell Nucleus: metabolism,Cercopithecus aethiops,Chromatography,Cytoplasm,Cytoplasm: metabolism,Detergents,Detergents: pharmacology,Fluorescence,Fluorescent Antibody Technique,Fluorescent Dyes,Fluorescent Dyes: pharmacology,HeLa Cells,High Pressure Liquid,Humans,Indirect,Mass,Matrix-Assisted Laser Desorpti,Microscopy,Neovascularization,Octoxynol,Octoxynol: pharmacology,Pathologic,Peptides,Peptides: chemistry,Protein Structure,Spectrometry,TMSB4X,Tertiary,Thymosin,Thymosin: chemistry,Transglutaminases,Transglutaminases: chemistry,Tumor,Vero Cells,Wound Healing,nucleus},
