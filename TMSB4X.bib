keywords = {Animals,Cell Differentiation,Cell Line, Tumor,Cell Movement,Cell Proliferation,Cells, Cultured,Colonic Neoplasms,Down-Regulation,Epithelial Cells,Gene Expression Regulation, Neoplastic,Humans,Lentivirus,Mice,Mice, SCID,Neoplastic Stem Cells,Oncogene Protein v-akt,PTEN Phosphohydrolase,Protein-Serine-Threonine Kinases,Thymosin,cytology,metabolism,physiology,physiopathology,virology},
language = {eng},
