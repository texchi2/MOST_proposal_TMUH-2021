issn = {0950-9232},
journal = {Oncogene},
keywords = {b -catenin,e-cadherin,epithelial,integrin-,linked kinase,mesenchymal transition,thymosin b 4},
number = {19},
pages = {2781--2790},
pmid = {17072345},
title = {{Thymosin beta4 triggers an epithelial-mesenchymal transition in colorectal carcinoma by upregulating integrin-linked kinase.}},
