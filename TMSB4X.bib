issn = {0006-3495},
journal = {Biophysical journal},
keywords = {Acrylamide,Acrylamide: chemistry,Actins,Actins: chemistry,Adenosine Triphosphate,Adenosine Triphosphate: chemistry,Animals,Calorimetry, Differential Scanning,Cysteine,Cysteine: chemistry,Electrophoresis, Polyacrylamide Gel,Fluorescence Resonance Energy Transfer,Fluorescence Resonance Energy Transfer: methods,Hot Temperature,Kinetics,Lysine,Lysine: chemistry,Models, Molecular,Molecular Conformation,Nucleotides,Nucleotides: chemistry,Protein Binding,Protein Conformation,Protein Structure, Tertiary,Purines,Purines: chemistry,Rabbits,Solvents,Solvents: chemistry,Spectrometry, Fluorescence,Temperature,Thymosin,Thymosin: chemistry,Thymosin: metabolism},
number = {3},
