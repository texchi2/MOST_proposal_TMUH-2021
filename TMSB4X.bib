keywords = {Actins,Animals,Contractile Proteins,Humans,Light,Microfilament Proteins,Polymers,Profilins,Rabbits,Radiation,Scattering,Thermodynamics,Thymosin,actin,chemistry,pharmacology,polymerisation},
language = {ENG},
