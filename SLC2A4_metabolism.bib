month = {mar},
number = {5},
pages = {1524--35},
pmid = {23288510},
title = {{Acidity generated by the tumor microenvironment drives local invasion.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23288510},
volume = {73},
year = {2013}
