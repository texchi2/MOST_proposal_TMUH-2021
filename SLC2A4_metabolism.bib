year = {2011}
}
@article{Petitjean2007,
abstract = {The tumor suppressor gene TP53 is frequently mutated in human cancers. More than 75{\%} of all mutations are missense substitutions that have been extensively analyzed in various yeast and human cell assays. The International Agency for Research on Cancer (IARC) TP53 database (www-p53.iarc.fr) compiles all genetic variations that have been reported in TP53. Here, we present recent database developments that include new annotations on the functional properties of mutant proteins, and we perform a systematic analysis of the database to determine the functional properties that contribute to the occurrence of mutational "hotspots" in different cancer types and to the phenotype of tumors. This analysis showed that loss of transactivation capacity is a key factor for the selection of missense mutations, and that difference in mutation frequencies is closely related to nucleotide substitution rates along TP53 coding sequence. An interesting new finding is that in patients with an inherited missense mutation, the age at onset of tumors was related to the functional severity of the mutation, mutations with total loss of transactivation activity being associated with earlier cancer onset compared to mutations that retain partial transactivation capacity. Furthermore, 80{\%} of the most common mutants show a capacity to exert dominant-negative effect (DNE) over wild-type p53, compared to only 45{\%} of the less frequent mutants studied, suggesting that DNE may play a role in shaping mutation patterns. These results provide new insights into the factors that shape mutation patterns and influence mutation phenotype, which may have clinical interest.},
author = {Petitjean, Audrey and Mathe, Ewy and Kato, Shunsuke and Ishioka, Chikashi and Tavtigian, Sean V and Hainaut, Pierre and Olivier, Magali},
doi = {10.1002/humu.20495},
issn = {1098-1004},
journal = {Human mutation},
keywords = {Age of Onset,DNA Mutational Analysis,Databases, Genetic,Germ-Line Mutation,Humans,IARC database,Mutation,Neoplasms,Neoplasms: genetics,Neoplasms: pathology,Phenotype,Transcriptional Activation,Transcriptional Activation: genetics,Tumor Suppressor Protein p53,Tumor Suppressor Protein p53: genetics,cancer,mutation,p53},
mendeley-tags = {IARC database,cancer,mutation,p53},
month = {jun},
number = {6},
pages = {622--9},
pmid = {17311302},
title = {{Impact of mutant p53 functional properties on TP53 mutation patterns and tumor phenotype: lessons from recent developments in the IARC TP53 database.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17311302},
volume = {28},
year = {2007}
}
