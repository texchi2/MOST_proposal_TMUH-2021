title = {{Present and Future of EGFR Inhibitors for Head and Neck Squamous Cell Cancer.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3321461{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {2012},
