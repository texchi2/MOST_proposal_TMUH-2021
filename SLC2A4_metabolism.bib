pages = {541--5},
pmid = {24670654},
publisher = {Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
shorttitle = {Nature},
title = {{Cell-cycle-regulated activation of Akt kinase by phosphorylation at its carboxyl terminus.}},
