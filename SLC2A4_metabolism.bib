mendeley-tags = {D.index,Survival},
month = {mar},
number = {5},
pages = {723--48},
pmid = {14981672},
title = {{A new measure of prognostic separation in survival data.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14981672},
volume = {23},
