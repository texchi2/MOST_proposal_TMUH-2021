journal = {Traffic (Copenhagen, Denmark)},
keywords = {Animals,Caveolae,Caveolae: metabolism,Cholesterol,Cholesterol: metabolism,Clathrin,Clathrin: metabolism,Dynamins,Dynamins: metabolism,Endocytosis,Glucose Transporter Type 4,Glucose Transporter Type 4: metabolism,Ion Channels,Ion Channels: chemistry,Mitochondria,Mitochondria: metabolism,Mitochondrial Proteins,Mitochondrial Proteins: chemistry,Models, Biological,Muscle Contraction,Myoblasts,Myoblasts: cytology,Myoblasts: metabolism,Rats,Trout},
month = {jul},
number = {7},
