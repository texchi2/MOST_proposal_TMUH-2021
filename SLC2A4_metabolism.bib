pages = {581},
pmid = {21114820},
publisher = {BioMed Central Ltd},
title = {{The INTERPRET Decision-Support System version 3.0 for evaluation of Magnetic Resonance Spectroscopy data from human brain tumours and other abnormal brain masses.}},
