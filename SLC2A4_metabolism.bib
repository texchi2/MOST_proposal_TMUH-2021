journal = {Cell},
keywords = {Animals,Cell Differentiation,Cell Lineage,Cell Transformation, Neoplastic,Chromatin,Chromatin: metabolism,Embryonic Stem Cells,Embryonic Stem Cells: metabolism,Enhancer Elements, Genetic,Feedback,Gene Expression Regulation, Developmental,Humans,Mice,Stem Cells,Stem Cells: metabolism},
month = {aug},
