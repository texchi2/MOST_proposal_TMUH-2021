pages = {1045--56},
pmid = {23972856},
title = {{A computational model of reactive oxygen species and redox balance in cardiac mitochondria.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23972856},
volume = {105},
year = {2013}
}
