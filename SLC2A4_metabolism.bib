journal = {Molecular cancer research : MCR},
keywords = {Adolescent,Adult,Aged,Bleomycin,Bleomycin: toxicity,Carcinoma, Squamous Cell,Carcinoma, Squamous Cell: genetics,Cells, Cultured,Female,Gene Expression Profiling,Gene Expression Regulation, Neoplastic,Gene Expression Regulation, Neoplastic: genetics,Genetic Predisposition to Disease,Head and Neck Neoplasms,Head and Neck Neoplasms: genetics,Humans,Lymphocytes,Lymphocytes: drug effects,Male,Middle Aged,Mutagens,Mutagens: toxicity,Neoplasm Proteins,Neoplasm Proteins: genetics,Oligonucleotide Array Sequence Analysis,Tumor Markers, Biological,Tumor Markers, Biological: genetics},
