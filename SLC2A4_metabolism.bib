file = {:Users/texchi/Library/Application Support/Mendeley Desktop/Downloaded/Niwa et al. - 2014 - IRAP Deficiency Attenuates Diet-induced Obesity in Mice Through Increased Energy Expenditure.pdf:pdf},
issn = {1090-2104},
journal = {Biochemical and biophysical research communications},
keywords = {AT1R,AT2R,AT4R,Ang,BAT,Brown adipose tissue,Diet-induced obesity,IRAP,IRAP GLUT4 UCP,Insulin-regulated aminopeptidase,PAI-1,PGC,PPAR,RAS,UCP,UCP-1,WAT,WT,angiotensin,angiotensin II type 1 receptor,angiotensin II type 2 receptor,angiotensin IV receptor,brown adipose tissue,insulin-regulated aminopeptidase,peroxisome proliferator-activated receptor,peroxisome proliferator-activated receptor-$\gamma$ coact,plasminogen activator inhibitor type 1,renin–angiotensin system,uncoupling protein,white adipose tissue,wild type},
