mendeley-tags = {GLUT4,simulation},
month = {jan},
number = {12},
pages = {e14217},
pmid = {21151967},
title = {{Molecular dynamics simulation studies of GLUT4: substrate-free and substrate-induced dynamics and ATP-mediated glucose transport inhibition.}},
