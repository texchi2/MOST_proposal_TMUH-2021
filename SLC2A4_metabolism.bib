journal = {Blood},
keywords = {Biological Availability,Cell Line,Cell Proliferation,Cell Survival,Cell Survival: genetics,Cells,Cultured,Disease Progression,Drug Evaluation,Facilitative,Facilitative: antagoni,Facilitative: genetics,Facilitative: metaboli,Facilitative: physiolo,GLUT11,Gene Expression Regulation,Glucose,Glucose Transport Proteins,Glucose Transporter Type 4,Glucose Transporter Type 4: antagonists {\&} inhibito,Glucose Transporter Type 4: genetics,Glucose Transporter Type 4: metabolism,Glucose Transporter Type 4: physiology,Glucose: metabolism,Glucose: pharmacokinetics,HIV Protease Inhibitors,HIV Protease Inhibitors: pharmacology,Humans,Molecular Targeted Therapy,Molecular Targeted Therapy: methods,Molecular Targeted Therapy: trends,Multiple Myeloma,Multiple Myeloma: genetics,Multiple Myeloma: metabolism,Multiple Myeloma: pathology,Multiple Myeloma: therapy,Neoplastic,Neoplastic: drug effec,Neoplastic: physiology,Off-Label Use,Preclinical,Primary Cell Culture,Ritonavir,Ritonavir: pharmacology,Tumor,cancer},
