journal = {PloS one},
keywords = {Biological Markers,Biological Markers: analysis,Databases,Factual,Gene Expression Profiling,Humans,Internet,Neoplasms,Neoplasms: metabolism,Neoplasms: mortality,OSCC,Survival Analysis,TCGA,survexpress,survival},
