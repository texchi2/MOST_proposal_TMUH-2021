@article{Yun2005,
abstract = {The induction of proangiogenic cytokines such as vascular endothelial growth factor (VEGF) is a critical feature of tumor angiogenesis. In the present study, we examined the mechanisms of VEGF gene expression induced by glucose deprivation in cancer cells, a role of AMP-activated protein kinase (AMPK) in the process, and the signal transduction pathway. AMPK functions as an energy sensor to provide metabolic adaptation under ATP-depleting conditions such as hypoxia and nutritional deprivation. Here, we show that glucose deprivation leads to a significant increase in the mRNA level of VEGF, GLUT1, and PFKFB3 genes in several cancer cells via a hypoxia-inducible factor-1-independent mechanism, and we demonstrate an essential role of AMPK in these gene expressions. Our data suggest that VEGF mRNA induction by glucose deprivation is due to an increase in mRNA stability, and the AMPK activity is necessary and sufficient to confer the stability to VEGF mRNA. We further show that reactive oxygen species is involved in glucose deprivation-induced AMPK activity in DU145 human prostate carcinomas, and c-Jun amino-terminal kinase acts as an upstream component in AMPK activation cascades under these conditions. LKB1, which was recently identified as a direct upstream kinase of AMPK, was not detected in DU145 cells. In conclusion, our results demonstrate a novel and major role of AMPK in the post-transcriptional regulation of VEGF, further implying its potential role in tumor angiogenesis. Previous Section Next Section As solid tumors outgrow the existing vasculature, they are continuously exposed to microenvironments where the supply of oxygen and nutrients are limited. Adaptation to such environments by induction of new blood vessels is critical to sustain neoplastic proliferation, and vascular endothelial growth factor (VEGF)1 plays a central role in this process (1, 2). A variety of human tumors and tumor cell lines express and secrete high levels of VEGF, which in turn induces proliferation, migration, and survival of endothelial cells. One of the best known environmental factors that up-regulate VEGF expression in tumors is hypoxia, and this induction occurs primarily via transcriptional activation by HIF-1 (a heterodimeric transcription factor composed of HIF-1$\alpha$ and HIF-1$\beta$) (3, 4). Its activity largely depends on accumulation of the oxygen-regulated subunit HIF-1$\alpha$, which is rapidly degraded under normoxic conditions and is stabilized under hypoxic conditions (3). Besides VEGF, several target genes, including GLUT1 (glucose transporter 1) and PFKFB3 (6-phosphofructo-2-kinase/fructose-2,6-bisphosphatase-3), are up-regulated by HIF-1 (3, 4). In concert with transcriptional regulation, hypoxia also increases the mRNA stability of VEGF, and the 3′-untranslated region (UTR) of the mRNA has been implicated in the regulation of its stability (5). In addition to a lack of oxygen delivery, glucose deprivation is another key factor to which tumors must respond and adapt as they outgrow the existing vasculature. In fact, several studies demonstrated that glucose deprivation also induces VEGF expression in different types of cancer cells (6–8), and a post-transcriptional regulation has been implicated under the existing conditions (6, 7). However, the precise mechanism as well as the regulatory components for low glucose-induced VEGF expression remain almost unknown. AMPK, a heterotrimer consisting of a catalytic subunit ($\alpha$) and two regulatory subunits ($\beta$ and $\gamma$), functions as an energy sensor in eukaryotic cells (9, 10). It is sensitively activated by allosteric binding of AMP, which is accumulated as a result of ATP depletion under various metabolic stresses such as hypoxia, nutrition deprivation, oxidative stress, and exercise. AMPK is also activated when it is phosphorylated by an upstream kinase. Upon activation, AMPK down-regulates several anabolic enzymes by phosphorylation and shuts down the ATP-consuming metabolic pathways, whereas its activation facilitates energy-producing pathways, thereby playing a major role in energy homeostasis. We have previously demonstrated that AMPK activity is critical for HIF-1 activity and its target gene expression such as VEGF under hypoxic conditions (11). In the present study, we investigated whether AMPK plays any role in the regulation of expression of genes that are critically involved in cancer adaptation and survival such as VEGF, GLUT1, and PFKFB3 under glucose-deprived conditions. In particular, our focus was on revealing the mechanisms of VEGF mRNA induction, a role of AMPK, and its signaling pathway. Here, we present novel evidence on the increase in VEGF mRNA stability through ROS-mediated activation of AMPK in DU145 cancer cells under glucose deprivation. Moreover, we reveal for the first time that JNK acts as an upstream signal component of AMPK in the signal pathway.},
author = {{Yun H Lee M}, Kim S S Ha J},
journal = {J Biol Chem},
month = {mar},
number = {11},
pages = {9963--9972},
title = {{Glucose Deprivation Increases mRNA Stability of Vascular Endothelial Growth Factor through Activation of AMP-activated Protein Kinase in DU145 Prostate Carcinoma.}},
url = {http://www.jbc.org/content/280/11/9963.full},
volume = {280},
year = {2005}
}
@article{Hancock2004,
abstract = {PURPOSE: To evaluate low-dose extended duration interferon alfa-2a as adjuvant therapy in patients with thick ({\textgreater} or = 4 mm) primary cutaneous melanoma and/or locoregional metastases. PATIENTS AND METHODS: In this randomized controlled trial involving 674 patients, the effect of interferon alfa-2a (3 megaunits three times per week for 2 years or until recurrence) on overall survival (OS) and recurrence-free survival (RFS) was compared with that of no further treatment in radically resected stage IIB and stage III cutaneous malignant melanoma. RESULTS: The OS and RFS rates at 5 years were 44{\%} (SE, 2.6) and 32{\%} (SE, 2.1), respectively. There was no significant difference in OS or RFS between the interferon-treated and control arms (odds ratio [OR], 0.94; 95{\%} CI, 0.75 to 1.18; P =.6; and OR, 0.91; 95{\%} CI, 0.75 to 1.10; P =.3; respectively). Male sex (P =.003) and regional lymph node involvement (P =.0009), but not age (P =.7), were statistically significant adverse features for OS. Subgroup analysis by disease stage, age, and sex did not show any clear differences between interferon-treated and control groups in either OS or RFS. Interferon-related toxicities were modest: grade 3 (and in only one case, grade 4) fatigue or mood disturbance was seen in 7{\%} and 4{\%} respectively, of patients. However, there were 50 withdrawals (15{\%}) from interferon treatment due to toxicity. CONCLUSION: The results from this study, taken in isolation, do not indicate that extended-duration low-dose interferon is significantly better than observation alone in the initial treatment of completely resected high-risk malignant melanoma.},
