pages = {271--306},
pmid = {19384426},
title = {{The phosphoinositide 3-kinase pathway in human cancer: genetic alterations and therapeutic implications.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2652403{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {8},
year = {2007}
}
