issn = {1932-6203},
journal = {PloS one},
keywords = {AMP-Activated Protein Kinases,AMP-Activated Protein Kinases: antagonists {\&} inhib,AMP-Activated Protein Kinases: genetics,AMP-Activated Protein Kinases: metabolism,AMPK,Animals,Autophagy,Autophagy: drug effects,Cattle,Cell Survival,Cell Survival: drug effects,Cell Survival: genetics,Cells, Cultured,Deoxyglucose,Deoxyglucose: pharmacology,Drug Evaluation, Preclinical,Endothelial Cells,Endothelial Cells: drug effects,Endothelial Cells: physiology,Enzyme Activation,Enzyme Activation: drug effects,Enzyme Activation: genetics,Gene Expression Regulation, Enzymologic,Gene Expression Regulation, Enzymologic: drug effe,Humans,RNA, Small Interfering,RNA, Small Interfering: pharmacology,ROS,Reactive Oxygen Species,Reactive Oxygen Species: metabolism,Reactive Oxygen Species: pharmacology,Up-Regulation,Up-Regulation: drug effects,mTOR},
