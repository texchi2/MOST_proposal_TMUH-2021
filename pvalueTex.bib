journal = {International journal of cancer},
keywords = {Adolescent,Adult,Aged,Child,Female,Follow-Up Studies,GLOBOCAN 2012,Global Health,Humans,Incidence,Infant,Male,Middle Aged,Mortality,Neoplasms,Newborn,Preschool,Prognosis,Registries,Risk Factors,Survival Rate,Young Adult,epidemiology,mortality,trends},
