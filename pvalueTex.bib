language = {English},
pages = {viii411},
title = {{A phase I study of MEDI1873, a novel GITR agonist, in advanced solid tumors}},
url = {https://www.embase.com/search/results?subaction=viewrecord{\&}id=L628559353{\&}from=export http://dx.doi.org/10.1093/annonc/mdy288.027},
volume = {29},
