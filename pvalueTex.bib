journal = {Frontiers in cell and developmental biology},
language = {eng},
pages = {304},
pmid = {31921836},
title = {{Thymosin $\beta$4 Regulates Focal Adhesion Formation in Human Melanoma Cells and Affects  Their Migration and Invasion.}},
volume = {7},
year = {2019}
}
