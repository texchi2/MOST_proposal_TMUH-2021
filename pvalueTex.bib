journal = {Oncology Research and Treatment},
keywords = {B lymphocyte,B lymphocyte receptor,Bruton tyrosine kinase,CD19 antigen,CD39 antigen,adenosine,adenosine triphosphate,adult,cancer patient,cell population,coculture,conference abstract,controlled study,endogenous compound,enzyme activity,exosome,flow cytometry,glucocorticoid induced tumor necrosis factor recep,head and neck squamous cell carcinoma,human,human cell,human tissue,hydrolysis,luciferase assay,phenotype,protein expression,signal transduction,size exclusion chromatography},
