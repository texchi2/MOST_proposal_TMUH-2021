month = {jul},
pages = {892},
publisher = {Frontiers Media S.A.},
title = {{CD56 in the Immune System: More Than a Marker for Cytotoxicity?}},
url = {https://pubmed.ncbi.nlm.nih.gov/28791027 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5522883/},
volume = {8},
